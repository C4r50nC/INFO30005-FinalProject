<h1>Leaderboard</h1>
<table class='table'>
  <thead>
    <tr>
      <th scope='col'>#</th>
      <th scope='col'>Screen Name</th>
      <th scope='col'>Engagement Rate</th>
    </tr>
  </thead>
  <tbody>
    {{#each patients}}
      <tr>
        <td>{{rank}}</td>
        <td>{{screenName}}</td>
        <td>{{engagementRateText}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>